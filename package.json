{
  "name": "guqin",
  "version": "0.0.1",
  "author": "Berg",
  "scripts": {
    "build": "webpack",
    "lint": "eslint '{app_model,app_service,middleware,system,util}/**/*.js'",
    "precommit": "npm run prettify && npm run lint",
    "prettify":
      "prettier --write --single-quote --trailing-comma all --loglevel silent '{app_model,app_service,middleware,system,util}/**/*.js'",
    "start": "npm run build && node ./dist/main",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "bluebird": "^3.5.1",
    "dotenv": "^6.0.0",
    "formidable": "^1.2.0",
    "hoek": "^5.0.3",
    "jsonwebtoken": "8.2.0",
    "koa": "2.5.0",
    "koa-bodyparser": "4.2.0",
    "koa-jwt": "3.3.1",
    "koa-logger": "3.2.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.1",
    "koa-static": "4.0.2",
    "log4js": "0.6.31",
    "moment": "^2.22.0",
    "mongoose": "^5.0.10",
    "redis": "^2.8.0",
    "serve-favicon": "^2.4.5",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "prettier": "^1.12.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-": "npm run precommit",
      "pre-push": "npm run prettify && npm run lint",
      "pre-rebase": "npm run prettify && npm run lint"
    }
  }
}
